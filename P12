print('Rainfall for Chicago 2017')

for x in range (0,12):
        number= [0]*12
        print(x)
        number[x]= float(input('Enter Rainfall: '))

len01=len(number)
print(number)
print('High Rainfall-> ', max(number))
print('Lowest Rainfall-> ', min(number))
print('Total Rainfall-> ', sum(number))
a=sum(number)/12
print('Average is: ', a)
